{
  "name": "savebills/laravel-budpay",
  "version": "1.0.0",
  "description": "Laravel package for interacting with BudPay APIs",
  "type": "library",
  "autoload": {
    "psr-4": {
      "BudPay\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "BudPay\\BudPayServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^7.3|^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/support": "^8.0|^9.0|^10.0"
  }
}



